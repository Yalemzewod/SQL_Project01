------ This query is my first project using sql ---

DROP TABLE IF EXISTS patient_demography;
-- Create table
CREATE TABLE patient_demography (
  patient_id VARCHAR(30) PRIMARY KEY,
  patient_name VARCHAR(30),
  addmission_reason VARCHAR(40),
  age INT,
  sex VARCHAR(30)
);
